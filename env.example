# TAO Magnet Bot Environment Configuration

# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/tao_magnet
REDIS_URL=redis://localhost:6379

# API Keys
BITTENSOR_RPC_URL=https://bittensor-rpc.com
BITTENSOR_API_KEY=your_bittensor_api_key
ETHEREUM_API_KEY=your_ethereum_api_key
SOLANA_API_KEY=your_solana_api_key
COINGECKO_API_KEY=your_coingecko_api_key

# Blockchain RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Bridge Configuration
LAYERZERO_ENDPOINT=0x66A71Dcef29A0fFBDBE3c6a460a3B5BC225Cd675
CHAINLINK_CCIP_ROUTER=0xE561d5E02207fb5eB32cca20a699E0d8919a1476

# Bot Configuration
MIN_PROFIT_THRESHOLD=10
MAX_SLIPPAGE=0.5
GAS_PRICE_MULTIPLIER=1.1
MAX_CONCURRENT_TRADES=3
RISK_TOLERANCE=0.7

# Trading Hours (24-hour format)
TRADING_HOURS_START=00:00
TRADING_HOURS_END=23:59

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3002

# Logging
LOG_LEVEL=info
LOG_FILE_PATH=./logs

# Security
JWT_SECRET=your_jwt_secret_key_here
ENCRYPTION_KEY=your_encryption_key_here

# Telegram Bot (Optional)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Email Notifications (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_email_password

# WebSocket
WS_PORT=3001
WS_PATH=/socket.io

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# CORS
CORS_ORIGIN=http://localhost:3000
CORS_CREDENTIALS=true 